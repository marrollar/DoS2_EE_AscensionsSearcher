generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./ascensions.db"
}

model derpys {
  aspect      String
  cluster     String
  node        String
  description String
  is_addition Int

  @@id([aspect, cluster, node])
}

model nodes {
  href         String
  aspect       String
  cluster      String
  attr         String
  description  String
  is_subnode   Int?
  has_implicit Int?

  @@id([aspect, cluster, attr])
}
